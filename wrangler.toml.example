name = "caldav-server"
main = "src/index.js"
compatibility_date = "2024-01-01"

[[d1_databases]]
binding = "DB"
database_name = "caldav_db"
database_id = "<your-database-id>"

[[kv_namespaces]]
binding = "KV"
id = "<your-kv-id>"

[[r2_buckets]]
binding = "R2"
bucket_name = "caldav-calendars"

[vars]
USERNAME = "admin"
PASSWORD = "changeme"
BASE_URL = "https://caldav-server.your-domain.workers.dev"

[observability]
enabled = false